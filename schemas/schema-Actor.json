{
    "title": "Actor",
    "description": "Entity Actor",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "description": "Actor's ID",
            "unique": true,
            "primary_key": true
        },
        "name": {
            "type": "string",
            "description": "Actor's Name",
            "faker": "name.findName",
            "maxLength": 50,
            "unique": false
        },
        "birthdate": {
            "type": "integer",
            "description": "Actor's Birthdate",
            "faker": "random.number",
            "maximum": "strftime('%s', datetime('now'))", 
            "unique": false
        },
        "date_death": {
            "type": "integer",
            "description": "Actor's Date of Death",
            "faker": "random.number",
            "minimum": "birthdate",
            "maximum": "strftime('%s', datetime('now'))",
            "unique": false
        }
        
    },
    "required": [
        "name","birthdate"
    ]
    

}