{
    "title": "Album",
    "description": "Entity Album",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "description": "Album's ID",
            "unique": true,
            "primary_key": true
        },
        "title": {
            "type": "string",
            "description": "Album's Title",
            "maxLength": 50, 
            "unique":false,
            "faker": "random.words"
        },
        "cover": {
            "type": "string",
            "description": "Album Cover Path",
            "image": true,
            "unique": true,
            "faker": "random.image"
        },
        "price": {
            "type": "number",
            "description": "Album's Price",
            "minimum": 0,
            "unique": false,
            "faker": "commerce.price"
        },
        "release_date": {
            "type": "integer",
            "description": "Album's Release",
            "minimum": "strftime('%s', datetime('now'))",
            "faker": "random.number",
            "unique": false
        }
    },
    "required": [
        "title", "cover", "price", "release_date"
    ]
    

}