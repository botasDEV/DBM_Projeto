{
    "title": "Movie",
    "description": "Entity Movie",
    "type": "object",
    "properties": {
        "id": {
            "type": "integer",
            "description": "Movie's ID",
            "unique": true,
            "primary_key": true
        },
        "title": {
            "type": "string",
            "description": "Movie's Title",
            "maxLength": 50, 
            "unique":false,
            "faker": "random.words"
        },
        "cover": {
            "type": "string",
            "description": "Movie Cover Path",
            "image": true,
            "unique": true,
            "faker": "random.image"
        },
        "price": {
            "type": "number",
            "description": "Movie's Price",
            "minimum": 0,
            "unique": false,
            "faker": "commerce.price"
        },
        "release_date": {
            "type": "string",
            "description": "Movie's Release",
            "faker": "date.past",
            "unique": false
        }
    },
    "required": [
        "title", "cover", "price", "release_date"
    ],
    "references": [{
        "model": "Actor",
        "relation": "M-M"
    },{
        "model": "Director",
        "relation": "1-1"
    },{
        "model": "Gender",
        "relation": "1-M"
    }]

}